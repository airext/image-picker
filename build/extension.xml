<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.github.airext.ImagePicker</id>
  <versionNumber>1.0.1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libImagePicker.a</nativeLibrary>
        <initializer>ANXImagePickerInitializer</initializer>
        <finalizer>ANXImagePickerFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libImagePickerSimulator.a</nativeLibrary>
        <initializer>ANXImagePickerInitializer</initializer>
        <finalizer>ANXImagePickerFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libImagePicker.jar</nativeLibrary>
        <initializer>com.github.airext.ImagePicker</initializer>
        <finalizer>com.github.airext.ImagePicker</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>libImagePicker.jar</nativeLibrary>
        <initializer>com.github.airext.ImagePicker</initializer>
        <finalizer>com.github.airext.ImagePicker</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment />
    </platform>
  </platforms>
</extension>